{
	"name": "adalab-web-starter-kit",
	"version": "4.0.0",
	"description": "Adalab web starter kit",
	"type": "module",
	"devDependencies": {
		"@vitejs/plugin-react": "^4.3.4",
		"@vituum/vite-plugin-concat": "^1.0.0",
		"sass": "^1.85.1",
		"sharp": "^0.32.6",
		"vite": "^6.2.2",
		"vite-plugin-html-inject": "^1.0.4",
		"vite-plugin-image-optimizer": "^1.1.7"
	},
	"scripts": {
		"start": "vite",
		"dev": "vite",
		"build": "vite build --emptyOutDir --outDir docs",
		"docs": "vite build --emptyOutDir --outDir docs",
		"preview": "vite preview",
		"push-docs": "npm run deploy",
		"deploy": "npm run build && git add docs -f && git commit -m 'Deploying to GitHub Pages' && git push"
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com:Adalab/adalab-web-starter-kit.git"
